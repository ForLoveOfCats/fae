import fae::stdout::print_string, print_newline

fn print(str: []u8) {
	print_string(str)
	print_newline()
}

fn main() {
	if true {
		if false {
			print("a")
		} else {
			print("1")
		}

		if true {
			print("b")
		} else {
			print("2")
		}
	}

	print("c")

	if false {
		print("d")
		if true {
			print("e")
		}
	} else if true {
		print("3")
	} else{
		print("4")
	}

	if true {
		print("f")
		if true {
			print("g")
			if false {
				print("5")
			} else if true {
				print("h")
			} else {
				print("6")
			}
			print("i")
		} else {
			print("7")
		}
		print("j")
	}

	print("k")
}
