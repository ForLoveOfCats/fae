import fae::stdout::print_string

fn main() {
	func(.None)
	func(.Some(.None))
	func(.Some(.Some(.A)))
	func(.Some(.Some(.B)))
}

fn func(val: Option<Option<Enum>>) {
	if val is Some and val is Some and val is B => print_string("It is B!\n")
	else => print_string("It is not B\n")
}

enum Enum {
	A
	B
}
