import fae::stdout::print_u8, print_f64, print_string

fn main() {
	function(Enum.A)
	function(Enum.B { field: 99.2 })
	function(Enum.C)
	function(Enum.D { other: b'A' })
}

fn function(arg: Enum) {
	match arg {
		C => print_string("C\n")

		B, A {
			print_f64(arg.field)
			print_string(" is B\n")
		}

		A {}

		C => print_string("other C\n")
	}
}

enum Enum {
	A
	B { field: f64}
	C
	D { other: u8 }
}
