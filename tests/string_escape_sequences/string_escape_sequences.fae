fn main() {
	print_cases([]{
		"hello there"
		"hello\nthere"
		"\nthere"
		"\n"
		"hello\n"
		"\nhello\n"
		"\r"
		"hello\r"
		"\rhello\r"
		"\r\n"
		"hello\r\n"
		"\r\nhello\r\n"
		"\t"
		"hello\t"
		"\thello\t"
		"\\hel\\lo\\"
		"hello\0there"
	})
}

fn print_cases(cases: []str) {
	mut index: isize = 0
	while index < cases.length {
		println(cases[index])
		index += 1
	}
}
