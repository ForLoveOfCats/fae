fn main() {
	case('A')
	case('\n')
	case('\r')
	case('\t')
	case('\\')
	case('\'')
	case('\0')

	println("")

	byte_case(b'A')
	byte_case(b'\n')
	byte_case(b'\r')
	byte_case(b'\t')
	byte_case(b'\\')
	byte_case(b'\'')
	byte_case(b'\0')
}

fn case(value: u32) {
	println(f"{value}")
}

fn byte_case(value: u8) {
	println(f"{value}")
}
