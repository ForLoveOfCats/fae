	println("")
	println("")

	// Slices
	let a = []i32 { //
		4 //
		//
		5 //
	}

	let b = []i32 { //
		//
		2 //
		6 //
		//
	}

	let c = []i32 { //

		9
	
		12
	
		13
		
	}

	for number in a => println(f"{number}")
	println("")
	for number in b => println(f"{number}")
	println("")
	for number in c => println(f"{number}")
}
