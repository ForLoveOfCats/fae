import fae::stdout::print_f64, print_string, print_newline

fn main() {
	let first: Enum = Enum.A
	let second: Enum = Enum.C { field: 42 }

	if first is C {
		print_string("First is C\n")
	} else {
		print_string("First is not C\n")
	}

	if second is C {
		print_string("Second is C: ")
		print_f64(second.field)
		print_newline()
	}

	first.check_c()
	second.check_c()
}

enum Enum {
	A
	B
	C { field: f64 }
}

method Enum
fn check_c() {
	if self is C {
		print_string("Self is C, divided: ")
		print_f64(self.field / 2)
		print_newline()
	}
}
