struct Large {
	a: i64
	b: i64
	c: i64
	d: i64
	e: i64
	f: i64
	g: i64
	h: i64
	i: i64
	j: i64
	k: i64
}

fn main() {
	let large = Large {
		a: 0
		b: 0
		c: 0
		d: 0
		e: 0
		f: 0
		g: 0
		h: 0
		i: 0
		j: 0
		k: 0
	}
	let number: f64 = 4

	println(f"{function(large, number)}")
	assert(large.a == 0)
}

fn function(mut large: Large, mut number: f64): f64 {
	large.a = 4
	assert(large.a == 4)
	number += 3
	return number
}
