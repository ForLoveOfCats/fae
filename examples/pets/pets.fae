import fae::stdout::print_string

enum VetSpecialty {
	Cat
	Dog
}

enum Treated {
	Yes
	No
}

enum Pet {
	name: []u8

	Cat
	Dog { hours_since_last_walk: i32 }
}

method Pet
fn visit_vet(specialty: VetSpecialty): Treated {
	return Treated.Yes
}

fn main() {
	let cat: Pet = Pet.Cat { name: "Lola" }
	let dog: Pet = Pet.Dog { name: "Odie", hours_since_last_walk: 3 }

	cat.visit_vet(VetSpecialty.Cat)
	dog.visit_vet(VetSpecialty.Cat)
}
