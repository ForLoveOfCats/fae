{
  "name": "vscode-fae",
  "displayName": "Fae Language",
  "description": "Support extension for the Fae programming language",
  "version": "0.0.2",
  "publisher": "fae-lang",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ForLoveOfCats/fae"
  },
  "contributes": {
    "languages": [{
      "id": "fae",
      "aliases": ["Fae", "fae"],
      "extensions": [".fae"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "fae",
      "scopeName": "source.fae",
      "path": "./syntaxes/fae.tmLanguage.json"
    }]
  }
}
