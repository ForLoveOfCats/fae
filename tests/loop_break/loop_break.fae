import fae::stdout::print_i32, print_newline

fn main() {
	print_case("A")
	while true {
		print_case("B")
		break
		print_case("C")
	}
	print_case("D")

	while true {
		print_case("E")
		while true {
			print_case("F")
			break
			print_case("G")			
		}
		print_case("H")
		break
		print_case("I")
	}

	mut index: i32 = 0
	while true {
		print_i32(index)
		print_newline()
		index += 1
		if index >= 5 {
			break
		}
	}
}

fn print_case(string: str) {
	import fae::stdout::print_string
	print_string(string)
	print_newline()
}
