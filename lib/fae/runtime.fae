export "main"
fn fae_main(argc: i32, argc: &&u8): i32 {
	::main()
	return 0
}

#lang "slice_bound_check_failure"
fn slice_bound_check_failure(length: isize, index: isize) {
	let stderr = fae::libc::StdErrFile.*

	fae::libc::fprintf(
		stderr
		"Index %ld is out of range of slice with length %ld\n".pointer
		index
		length
	)
	fae::libc::fflush(stderr)

	fae::libc::exit(-1)
}
