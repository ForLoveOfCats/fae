fn logical_and(a: bool, b: bool, letter: str) {
	if a and b {
		println(letter)
	}
}

fn logical_or(a: bool, b: bool, letter: str) {
	if a or b {
		println(letter)
	}
}

fn main() {
	if true and false {
		println("A")
	}

	if true and true {
		println("B")
	}

	if false and true {
		println("C")
	}

	if false and false {
		println("D")
	}


	if true or false {
		println("E")
	}

	if true or true {
		println("F")
	}

	if false or true {
		println("G")
	}

	if false or false {
		println("H")
	}

	logical_and(true, false, "I")
	logical_and(true, true, "J")
	logical_and(false, true, "K")
	logical_and(false, false, "L")

	logical_or(true, false, "M")
	logical_or(true, true, "N")
	logical_or(false, true, "O")
	logical_or(false, false, "P")
}

