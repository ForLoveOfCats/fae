generic T
fn identity(arg: T): T {
	return arg
}

fn main() {
	let a: i32 = -5
	println(f"{a}")

	let b: u64 = --5
	println(f"{b}")

	let c: f64 = -4.2
	println(f"{c}")

	let d: f32 = --4.5
	println(f"{d}")

	println(f"{--identity<i32>(2)}")

	let e = identity<bool>(false).!
	println(f"{e}")

	println(f"{false.!.!}")
}
