import fae::stdout::print_isize, print_newline

struct Empty {}

struct ContainsEmpty {
	a: Empty
	b: i32
	c: Empty
}

struct MultipleFields {
	a: i32
	b: u8
	c: u64
	d: u8
}

struct Nested {
	a: MultipleFields
	b: u8
}

generic T
fn generic_func() {
	print_isize(alignment_of<T>())
}

fn main() {
	print_isize(alignment_of<u32>()) // 4
	print_newline()

	print_isize(alignment_of<u64>()) // 8
	print_newline()

	print_isize(alignment_of<i8>()) // 1
	print_newline()

	print_isize(alignment_of<f32>()) // 4
	print_newline()

	print_isize(alignment_of<Empty>()) // 0
	print_newline()

	print_isize(alignment_of<ContainsEmpty>()) // 4
	print_newline()

	print_isize(alignment_of<MultipleFields>()) // 8
	print_newline()

	print_isize(alignment_of<Nested>()) // 8
	print_newline()

	generic_func<u8>() // 1
	print_newline()

	generic_func<isize>() // 8
	print_newline()

	generic_func<u16>() // 2
	print_newline()
}
