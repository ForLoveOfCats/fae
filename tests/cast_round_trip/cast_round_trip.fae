fn main() {
	let pointer = 999.(i64).(&void)
	let other = pointer.(&i32)
	println(f"{other.(u64)}")


	let a: i32 = -42
	let b = a.(u64)
	println(f"{b}")
	println(f"{b.(i32)}")

	let c = a.(f32)
	println(f"{c}")
	println(f"{a.(f64)}")

	println(f"{c.(i32)}")
	println(f"{c.(i64)}")

	println(f"{c.(f64)}")
	println(f"{4.9.(f64).(f32)}")
}
