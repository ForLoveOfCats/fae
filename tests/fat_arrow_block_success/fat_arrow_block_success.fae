import fae::stdout::print_i32, print_string, print_newline

fn main() {
	mut index: i32 = 0
	while index < 10 => index += 1
	print_i32(index)
	print_newline()

	if false => print_string("if\n")
	else if false => print_string("else if 1\n")
	else if true => print_string("else if 2\n")
	else => print_string("else\n")

	print_string("After everything\n")
}
