fn main() {
	// Arrays
	let a = [_]i32 { //
		11 //
		//
		12 //
	}

	let b = [_]i32 { //
		//
		15 //
		16 //
		//
	}

	let c = [_]i32 { //

		20
	
		21
	
		22
		
	}

	for number in a => println(f"{number}")
	println("")
	for number in b => println(f"{number}")
	println("")
