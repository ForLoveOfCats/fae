import fae::stdout::print_u8, print_f64, print_string, print_newline

fn main() {
	func(Enum.A)
	func(Enum.B { field: 99.2 })
	func(Enum.C)
	func(Enum.D { other: b'A' })
}

fn func(arg: Enum) {
	if arg is A, D, A {
		print_u8(arg.other)
		print_newline()
		print_string("Is either A or D\n")
	} else {
		print_string("Is something other than A or D\n")
	}
}

enum Enum {
	A
	B { field: f64}
	C
	D { other: u8 }
}
