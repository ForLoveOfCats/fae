fn main() {
	func(Enum.A)
	func(Enum.B { field: 99.2 })
	func(Enum.C)
	func(Enum.D { other: b'A' })
}

fn func(arg: Enum) {
	if arg is A, D, A {
		println(f"{arg.other}")
		println("Is either A or D")
	} else {
		println("Is something other than A or D")
	}
}

enum Enum {
	A
	B { field: f64}
	C
	D { other: u8 }
}
