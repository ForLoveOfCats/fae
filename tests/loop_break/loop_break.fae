fn main() {
	println("A")
	while true {
		println("B")
		break
		println("C")
	}
	println("D")

	while true {
		println("E")
		while true {
			println("F")
			break
			println("G")			
		}
		println("H")
		break
		println("I")
	}

	mut index: i32 = 0
	while true {
		println(f"{index}")
		index += 1
		if index >= 5 {
			break
		}
	}
}
