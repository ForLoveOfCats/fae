fn main() {
	func(Enum.A)
	func(Enum.B { field: 99.2 })
	func(Enum.C)
	func(Enum.D { other: b'A' })
}

fn func(arg: Enum) {
	match arg {
		A, C => println("A or C")

		B {
			println(f"{arg.field} is B")
		}

		d: D {
			println(f"{d.other} is D")
		}
	}
}

enum Enum {
	A
	B { field: f64}
	C
	D { other: u8 }
}
