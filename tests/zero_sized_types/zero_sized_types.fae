import fae::stdout::print_i32

struct A {}

struct ContainsA {
	a: A
}

struct ContainsMultiple {
	first: ContainsA
	second: i32
}

fn func_a(a: A): A {
	return a
}

fn func_b(c: ContainsMultiple): ContainsA {
	let contains = c.first
	return contains
}

fn main() {
	mut a: A = A {}
	mut b = ContainsA { a: a }
	let multiple = ContainsMultiple { first: b, second: -3 }
	b = func_b(multiple)
	b.a = A {}
	a = b.a
	print_i32(multiple.second)
}
